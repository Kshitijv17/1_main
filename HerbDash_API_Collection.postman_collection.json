							"host": ["{{base_url}}"],
							"path": ["v1", "products", "search"],
							"query": [
								{"key": "q", "value": "herb"},
								{"key": "category_id", "value": ""},
								{"key": "shop_id", "value": ""},
								{"key": "min_price", "value": ""},
								{"key": "max_price", "value": ""},
								{"key": "sort_by", "value": "relevance"}
