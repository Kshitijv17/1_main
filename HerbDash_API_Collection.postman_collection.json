						},
						"url": "{{base_url}}/v1/cart/1"
					}
				},
				{
					"name": "Remove Cart Item",
					"request": {
						"method": "DELETE",
						"url": "{{base_url}}/v1/cart/1"
					}
				},
				{
					"name": "Clear Cart",
					"request": {
						"method": "DELETE",
						"url": "{{base_url}}/v1/cart"
					}
				}
			]
		},
